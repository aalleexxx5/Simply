echo Threaded _ advanced _ example
set a 1
set b 1
subroutine alpha
section a
echo subroutine1 _ says _ *a
add a *a 1
wait 500
if *a < 10 jumpto a
endsubroutine
subroutine beta
section b
echo subroutine2 _ says _ *b
add b 1 *b
wait 500
if *b < 10 jumpto b
jumpto bDone
endsubroutine
run alpha
run beta
echo ping
exit
section bDone
echo b _ is _ done
wait 1000
set a 4
run alpha
run alpha
exit
